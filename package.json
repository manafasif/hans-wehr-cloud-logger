    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.3.0",
    "mongoose": "^7.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "sqlite3": "^5.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
